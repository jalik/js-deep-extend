{
  "name": "@jalik/deep-extend",
  "version": "3.0.0",
  "description": "Deep merge any objects",
  "license": "MIT",
  "keywords": [
    "extend",
    "inherit",
    "merge"
  ],
  "author": {
    "name": "Karl Stein",
    "email": "karl.stein.pro@gmail.com",
    "url": "https://github.com/jalik"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jalik/js-deep-extend.git"
  },
  "bugs": {
    "url": "https://github.com/jalik/js-deep-extend/issues"
  },
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs/",
    "esm/",
    "LICENSE.txt",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-esm",
    "build-cjs": "tsc -p tsconfig.cjs.json",
    "build-esm": "tsc",
    "clean": "rimraf ./cjs ./esm",
    "dev": "tsc --watch",
    "lint": "eslint --ext .ts .",
    "lint-fix": "eslint --ext .ts --fix .",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint && npm test",
    "test": "vitest run --passWithNoTests --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
